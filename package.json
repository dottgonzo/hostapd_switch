  "license": "GPL v2",
  "bugs": {
    "url": "https://github.com/dottgonzo/hostapd_switch/issues"
  },
  "dependencies": {
    "bluebird": "^3.1.1",
    "dnsmasq-conf": "0.0.10",
    "hostapdjs": "0.0.15",
    "json-add": "0.0.7",
    "netw": "0.0.52",
    "path-exists": "^2.1.0",
    "promise-test-connection": "0.0.27",
    "promised-exec": "^1.0.1",
    "verbo": "0.0.9"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "gulp": "^3.9.0",
    "gulp-bump": "^1.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-filter": "^3.0.1",
    "gulp-git": "^1.6.0",
    "gulp-mocha": "^2.2.0",
    "gulp-prompt": "^0.1.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tag-version": "^1.3.0",
