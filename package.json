{
  "name": "hostapd_switch",
  "version": "0.0.31",
  "description": "Utility to easy switch between client mode and ap mode. ",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "ap": "node ./run/ap.js",
    "host": "node ./run/host.js",
    "client": "node ./run/client.js",
    "settings": "node ./run/settings.js",
    "hw": "git add -A && git commit -m 'oki'&&npm version patch -m patch&&git add -A && git commit --amend -C HEAD&& git push&&npm publish",
    "patch": "npm version patch -m patch&&git add -A && git commit --amend -C HEAD&& git push&&npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dottgonzo/hostapd_switch.git"
  },
  "keywords": [
    "network",
    "hostapd",
    "wpa_supplicant",
    "wpasupplicant",
    "wifi",
    "wlan"
  ],
  "author": "Dario Caruso",
  "license": "GPL v2",
  "bugs": {
    "url": "https://github.com/dottgonzo/hostapd_switch/issues"
  },
  "dependencies": {
    "dnsmasq-conf": "0.0.5",
    "hostapdjs": "0.0.3",
    "json-add": "0.0.1",
    "netw": "0.0.18",
    "path-exists": "^2.1.0",
    "promise": "^7.0.4",
    "promise-test-connection": "0.0.2",
    "promised-exec": "^1.0.1",
    "verbo": "0.0.9",
    "waitfor-promise": "0.0.3"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "mocha": "^2.3.4"
  }
